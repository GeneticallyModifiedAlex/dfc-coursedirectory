@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers

@model Dfc.CourseDirectory.Web.ViewComponents.EditVenueName.EditVenueNameModel

<div id="edit-venue">
        <div class="govuk-grid-row">
            <div class="govuk-grid-column-full">
                
                    <div class="govuk-form-group" style="clear:both;">
                        <div class="govuk-form-group">
                            <div class="govuk-form-group">
                                <label class="govuk-label" asp-for="VenueName"></label>
                                <span asp-validation-for="VenueName"></span>
                                <input asp-for="VenueName" value="@Model.VenueName" 
                                    class="govuk-input" 
                                    type="text"  
                                    autocomplete="off" />
                            </div>
                        </div>
                    </div>
                    <div class="govuk-form-group">
                        <button type="submit" class="govuk-button">Continue</button>
                    </div>
                
            </div>
        </div>
</div>



