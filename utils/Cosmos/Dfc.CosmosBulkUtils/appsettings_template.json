{
  "CosmosDb": {
    "EndpointUrl": "[INSERT_COSMOS_ENDPOINT_URL]",
    "AccessKey": "[INSERT_COSMOS_ACCESS_KEY]",
    "ContainerId": "[INSERT_CONTAINER_NAME]",
    "DatabaseId": "[INSERT_DATABASE_NAME]"
  },

  "Serilog": {
    "Using": [],
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Debug",
        "System": "Warning"
      }
    },
    "Enrich": [ "FromLogContext" ],
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "File",
        "Args": {
          "path": ".\\Log.txt"
        }
      }
    ]

  }
}
